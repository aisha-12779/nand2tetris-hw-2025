/** 
 * Multiplexor:
 * if (sel = 0) out = a, else out = b
 */
CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
    Nand(a=sel,b=sel,out=snot);
    Nand(a=snot,b=b,out=w1);
    Nand(a=a,b=sel,out=w2);
    Nand(a=w1,b=w2,out=mux);
}